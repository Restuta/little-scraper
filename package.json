{
  "name": "little-scraper",
  "version": "2.1.0",
  "description": "Little Node Scraper with simple by powerful async flow control goodness",
  "files": [
    "index.js",
    "README.md",
    "src"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production",
    "dev": "nodemon index.dev.js --ext js",
    "prestart": "npm run build",
    "start": "node index.js",
    "reinstall": "rimraf node_modules && yarn",
    "release:patch": "npm version patch && git push --all && git push --tags",
    "release:minor": "npm version minor && git push --all && git push --tags",
    "release": "npm run build && np"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Restuta/little-scraper.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "scraper"
  ],
  "author": "Anton Vynogradenko <restuta8@gmail.com> (http://restuta.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Restuta/little-scraper/issues"
  },
  "homepage": "https://github.com/Restuta/little-scraper#readme",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "cheerio": "^0.22.0",
    "cross-env": "^3.1.4",
    "express": "^4.16.2",
    "jsverify": "^0.7.4",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "np": "^2.16.0",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.0",
    "snazzy": "^6.0.0",
    "source-map-support": "^0.4.11",
    "standard": "^8.6.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "DEV",
      "PROD",
      "TEST"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "bluebird": "^3.4.0",
    "chalk": "^2.1.0",
    "lodash": "^4.12.0",
    "moment": "^2.10.6",
    "ramda": "^0.25.0",
    "request": "^2.48.0",
    "rxjs": "^5.5.4"
  }
}
